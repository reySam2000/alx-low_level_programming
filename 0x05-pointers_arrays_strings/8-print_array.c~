#include <stdio.h>
#include "main.h"

/**
 * print_array - prints n element array of integers
 * and a new line
 * @a: printed array
 * @n: printed elements
 */

void print_array(int *a, int n)
{
	int x;

	for (x = 0 ; x < n ; x++)
	{
		if (x == 0)
			printf("%d", a[x]);
		else
			printf(", %d", a[x]);
	}
	printf("\n");;
}
